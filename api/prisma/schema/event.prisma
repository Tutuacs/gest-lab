model Event {
  id                 Int        @id @default(autoincrement())
  name               String
  description        String
  from               DateTime
  to                 DateTime
  eventType          EVENT_TYPE
  ActivateLicense    Boolean    @default(false)
  DesactivateLicense Boolean    @default(false)
  value              Float      @default(0)
  createdAt          DateTime   @default(now())

  Equipament Equipament[]
}
